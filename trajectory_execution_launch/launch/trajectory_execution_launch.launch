<?xml version="1.0"?>
<launch>
	<arg name="smooth" default="false"/>
	<arg name="clean" default="true"/>
	<arg name="control" default="true"/>


	<include file="$(find moveit_trajectory_execution)/launch/moveit_trajectory_execution.launch" unless="$(arg control)"/>
	<include file="$(find control_trajectory_execution)/launch/control_trajectory_execution.launch" if="$(arg control)"/>

	<include file="$(find trajectory_preprocessing)/launch/trajectory_preprocessing_launch.launch">
		<arg name="smooth" default="$(arg smooth)"/>
		<arg name="clean" default="$(arg clean)"/>
	</include>
</launch>