<?xml version="1.0"?>
<launch>
	<node name="trajectory_csv_example_node" type="trajectory_csv_example.py" pkg="use_case_traj_exec" args="$(find use_case_traj_exec)/csv/trajectory_example.csv" output="screen" respawn="false">
	</node>

	<node name="control_trajectory_execution_action_server" type="control_trajectory_execution_action_server" pkg="control_trajectory_execution" output="screen" respawn="false">
		<rosparam file="$(find use_case_traj_exec)/config/control_trajectory_execution_action_server.yaml" command="load"/>
	</node>

	<node name="control_trajectory_execution_action_client" type="control_trajectory_execution_action_client" pkg="use_case_traj_exec" output="screen" respawn="false">
		<rosparam file="$(find use_case_traj_exec)/config/control_trajectory_execution_action_client.yaml" command="load"/>
	</node>
</launch>
